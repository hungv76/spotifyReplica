{
    "extends": "airbnb",
    "rules": {
        "react/prefer-stateless-function": "off",
        "react/prop-types": "off",
        "no-console": "off",
        "react/jsx-filename-extension": "off",
        "prefer-const": "off",
        "arrow-body-style": "off",
        "class-methods-use-this":"off",
        "object-curly-newline": "off"
    },
    "parser": "babel-eslint",
    "parserOptions": {
        "sourceType": "module",
        "allowImportExportEverywhere": false,
        "codeFrame": false
    }
}